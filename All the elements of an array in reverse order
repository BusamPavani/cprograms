#include <stdio.h>
int main() {
    int a[10],b[0],n,i,rev=0;
    printf("Enter size:");
    scanf("%d",&n);
    printf("Enter the elements:");
    for(i=0;i<n;i++)
    {
        scanf("%d",&a[i]);
    }
      for(i=n-1;i>0;i--)
      {
       printf("%d",a[i]);
      }
      printf("The reverse order is %d",rev);
}
