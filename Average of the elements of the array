#include <stdio.h>
int main() {
    int a[10],n,i,sum=0,avg;
    printf("Enter size:");
    scanf("%d",&n);
    printf("Enter the elements:");
    for(i=0;i<n;i++)
    {
        scanf("%d",&a[i]);
    }
      for(i=0;i<n;i++)
      {
          sum=sum+a[i];
      }
      avg=sum/n;
      printf("The average of the elements of the array is %d",avg);
}
