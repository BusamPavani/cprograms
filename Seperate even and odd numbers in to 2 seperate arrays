#include <stdio.h>
int main() {
    int a[10],n,i,b[10],c[10],j=0,k=0;
    printf("Enter size:");
    scanf("%d",&n);
    printf("Enter the elements:");
    for(i=0;i<n;i++)
    {
        scanf("%d",&a[i]);
    }
      for(i=0;i<n;++i)
      {
       if(a[i]%2==0)
       {
           b[j]=a[i];
           j++;
       }
       else
       {
           c[k]=a[i];
           k++;
       }
      }
       for(i=0;i<j;++i)
       {
           printf("%d",b[i]);
       }
       for(i=0;i<k;++i)
       {
      printf(" %d",c[i]);
}
}


